{
  "name": "@onchainfi/uplink",
  "version": "1.0.0",
  "description": "Access Point to the Onchain Aggregator - JavaScript/TypeScript SDK",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsx examples/simple-payment.ts",
    "test": "tsx test-uplink.ts",
    "test:samechain": "tsx test-samechain.ts",
    "test:base": "tsx examples/base-payment.ts",
    "test:solana": "tsx examples/solana-payment.ts"
  },
  "keywords": [
    "x402",
    "payments",
    "ai-agents",
    "usdc",
    "solana",
    "ethereum",
    "base"
  ],
  "author": "0xvsr",
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/onchainfi/uplink"
  },
  "dependencies": {
    "ethers": "^6.13.0",
    "@solana/web3.js": "^1.98.4",
    "@solana/spl-token": "^0.4.14",
    "bs58": "^6.0.0"
  },
  "devDependencies": {
    "typescript": "^5.6.0",
    "@types/node": "^20.0.0",
    "tsx": "^4.19.0",
    "tsup": "^8.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}

